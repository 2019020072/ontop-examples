PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/> 
PREFIX bsbm-inst: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/>
PREFIX bsbm-inst-prod: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer12/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX dc: <http://purl.org/dc/elements/1.1/> 
PREFIX rev: <http://purl.org/stuff/rev#> 
PREFIX foaf: <http://xmlns.com/foaf/0.1/> 
SELECT DISTINCT ?offer ?price 
WHERE {  
    ?offer bsbm:product bsbm-inst-prod:Product1985 . 
    ?offer bsbm:vendor ?vendor . 
    ?offer dc:publisher ?vendor . 
    ?vendor bsbm:country <http://downlode.org/rdf/iso-3166/countries#RU> . 
    ?offer bsbm:deliveryDays ?deliveryDays . 
  FILTER (?deliveryDays < 5) 
    ?offer bsbm:price ?price . 
    ?offer bsbm:validTo ?date . 
}
ORDER BY ?price 
LIMIT 10
