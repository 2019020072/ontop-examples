PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/> 
PREFIX bsbm-export: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/export/> 
PREFIX bsbm-inst-vend: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromVendor/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX dc: <http://purl.org/dc/elements/1.1/> 
PREFIX rev: <http://purl.org/stuff/rev#> 
PREFIX foaf: <http://xmlns.com/foaf/0.1/> 
SELECT ?productURI ?productlabel ?vendorname ?vendorhomepage ?offerURL ?price ?deliveryDays ?validTo 
WHERE {  
    bsbm-inst-vend:Offer5300215 bsbm:product ?productURI . 
    ?productURI rdfs:label ?productlabel . 
    bsbm-inst-vend:Offer5300215 bsbm:vendor ?vendorURI . 
    ?vendorURI rdfs:label ?vendorname . 
    ?vendorURI foaf:homepage ?vendorhomepage . 
    bsbm-inst-vend:Offer5300215 bsbm:offerWebpage ?offerURL . 
    bsbm-inst-vend:Offer5300215 bsbm:price ?price . 
    bsbm-inst-vend:Offer5300215 bsbm:deliveryDays ?deliveryDays .
    bsbm-inst-vend:Offer5300215 bsbm:validTo ?validTo
}
