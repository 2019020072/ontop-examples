{
  "constraints": {
    "non-unique-functional": [
      {
        "collection": "products",
        "determinants": [
          "producer.producerId"
        ],
        "dependents": [
          "producer.name",
          "producer.country",
          "producer.homepage"
        ]
      },

      {
        "collection": "products",
        "determinants": [
          "features.featureId"
        ],
        "dependents": [
          "features.description",
          "features.comment"
        ]
      },
      {
        "collection": "products",
        "determinants": [
          "offers.offerId"
        ],
        "dependents": [
          "offers.price",
          "offers.timeSpan.from",
          "offers.timeSpan.to",
          "offers.deliveryDays",
          "offers.offerWebpage",
          "offers.vendor.vendorId",
          "offers.vendor.name",
          "offers.vendor.homepage",
          "offers.vendor.country"
        ]
      },
      {
        "collection": "products",
        "determinants": [
          "offers.vendor.vendorId"
        ],
        "dependents": [
          "offers.vendor.country",
          "offers.vendor.name",
          "offers.vendor.homepage"
        ]
      },
      {
        "collection": "products",
        "determinants": [
          "reviews.reviewer.personId"
        ],
        "dependents": [
          "reviews.reviewer.name",
          "reviews.reviewer.country"
        ]
      }
    ],
    "unique": [
      {
        "collection": "products",
        "determinants": [
          "reviews.reviewId"
        ]
      }
    ]
  }
}
